{
  "include" : [
    "_FrameworkTemplate.yml",
    "_UnitTestTemplate.yml",
    "_UITestTemplate.yml",
    "_AggregateTargets.yml"
  ],
  "settings" : {
    "base" : {
      "SWIFT_VERSION" : 5.2999999999999998,
      "SWIFT_OBJC_BRIDGING_HEADER" : "Application\/SmartApp-Bridging-Header.h",
      "MARKETING_VERSION" : 1,
      "IPHONEOS_DEPLOYMENT_TARGET" : 16,
      "CLANG_ANALYZER_LOCALIZABILITY_NONLOCALIZED" : true
    }
  },
  "targets" : {
    "Common" : {
      "templates" : [
        "Framework"
      ],
      "settings" : {
        "base" : {
          "MARKETING_VERSION" : 1,
          "SWIFT_OBJC_BRIDGING_HEADER" : "Common\/Sources\/Common-Bridging-Header.h"
        }
      },
      "sources" : [
        {
          "path" : "..\/Common",
          "excludes" : [
            "Tests",
            "Sources\/Info.plist",
            ".gitignore",
            ".swiftpm",
            "Package.swift",
            "Package.resolved"
          ]
        }
      ]
    },
    "DesignSystem" : {
      "sources" : [
        {
          "excludes" : [
            "Info.plist"
          ],
          "path" : "..\/DesignSystem"
        }
      ],
      "dependencies" : [
        {
          "target" : "Common"
        },
        {
          "target" : "Domain"
        },
        {
          "target" : "DevTools"
        }
      ],
      "templates" : [
        "Framework"
      ],
      "settings" : {
        "base" : {
          "MARKETING_VERSION" : 1
        }
      }
    },
    "Core" : {
      "templates" : [
        "Framework"
      ],
      "settings" : {
        "base" : {
          "MARKETING_VERSION" : 1
        }
      },
      "dependencies" : [
        {
          "target" : "Domain"
        },
        {
          "target" : "DevTools"
        },
        {
          "target" : "Common"
        }
      ],
      "sources" : [
        {
          "path" : "..\/Core",
          "excludes" : [
            "Info.plist"
          ]
        }
      ]
    },
    "DevTools" : {
      "templates" : [
        "Framework"
      ],
      "settings" : {
        "base" : {
          "MARKETING_VERSION" : 1
        }
      },
      "dependencies" : [
        {
          "target" : "Common"
        }
      ],
      "sources" : [
        {
          "excludes" : [
            "Info.plist"
          ],
          "path" : "..\/DevTools"
        }
      ]
    },
    "Domain" : {
      "templates" : [
        "Framework"
      ],
      "settings" : {
        "base" : {
          "MARKETING_VERSION" : 1
        }
      },
      "sources" : [
        {
          "excludes" : [
            "Info.plist"
          ],
          "path" : "..\/Domain"
        }
      ],
      "dependencies" : [
        {
          "target" : "Common"
        },
        {
          "target" : "DevTools"
        }
      ]
    },
    "SmartAppUITests" : {
      "templates" : [
        "UITest"
      ],
      "sources" : [
        {
          "path" : "..\/SmartAppUITests",
          "excludes" : [
            "Info.plist"
          ]
        }
      ],
      "dependencies" : [
        {
          "sdk" : "XCTest.framework"
        },
        {
          "target" : "SmartApp",
          "link" : true
        },
        {
          "link" : false,
          "target" : "Common"
        },
        {
          "package" : "Nimble",
          "link" : true,
          "product" : "Nimble"
        },
        {
          "package" : "ViewInspector",
          "product" : "ViewInspector",
          "link" : true
        }
      ]
    },
    "SmartApp" : {
      "type" : "application",
      "dependencies" : [
        {
          "target" : "Common"
        },
        {
          "target" : "DevTools"
        },
        {
          "target" : "Domain"
        },
        {
          "target" : "Core"
        },
        {
          "target" : "DesignSystem"
        },
        {
          "product" : "Reachability",
          "package" : "Reachability",
          "link" : false
        },
        {
          "package" : "Firebase",
          "product" : "FirebaseMessaging",
          "link" : true
        },
        {
          "package" : "Firebase",
          "product" : "FirebaseCrashlytics",
          "link" : true
        },
        {
          "product" : "FirebaseRemoteConfig",
          "package" : "Firebase",
          "link" : true
        },
        {
          "package" : "Firebase",
          "link" : true,
          "product" : "FirebaseAnalytics"
        },
        {
          "package" : "Firebase",
          "link" : true,
          "product" : "FirebaseAuth"
        },
        {
          "link" : true,
          "package" : "Firebase",
          "product" : "FirebaseDatabase"
        },
        {
          "package" : "Firebase",
          "link" : true,
          "product" : "FirebaseAppCheck"
        }
      ],
      "platform" : "iOS",
      "settings" : {
        "base" : {
          "TARGETED_DEVICE_FAMILY" : 1,
          "SUPPORTED_PLATFORMS" : "iphoneos iphonesimulator",
          "SUPPORTS_MACCATALYST" : false,
          "SUPPORTS_MAC_DESIGNED_FOR_IPHONE_IPAD" : false
        }
      },
      "configFiles" : {
        "Dev Debug" : "..\/Configuration\/dev-debug.xcconfig",
        "QA Debug" : "..\/Configuration\/qa-debug.xcconfig",
        "QA Release" : "..\/Configuration\/qa-release.xcconfig",
        "Dev Release" : "..\/Configuration\/dev-release.xcconfig",
        "Production Release" : "..\/Configuration\/production-release.xcconfig",
        "Production Debug" : "..\/Configuration\/production-debug.xcconfig"
      },
      "scheme" : {
        "configVariants" : [
          "Production",
          "QA",
          "Dev"
        ],
        "testTargets" : [
          "SmartAppUnitTests",
          "SmartAppUITests"
        ]
      },
      "sources" : [
        {
          "type" : "group",
          "excludes" : [
            "*.md",
            "*.png"
          ],
          "path" : "..\/Application"
        }
      ],
      "postCompileScripts" : [
        {
          "name" : "FirebasePlistFile",
          "script" : "PATH_TO_CONFIG=$SRCROOT\/Application\/GoogleService-Info-$PRODUCT_BUNDLE_IDENTIFIER.plist\nFILENAME_IN_BUNDLE=GoogleService-Info.plist\nBUILD_APP_DIR=${BUILT_PRODUCTS_DIR}\/${PRODUCT_NAME}.app\ncp $PATH_TO_CONFIG \"$BUILD_APP_DIR\/$FILENAME_IN_BUNDLE\"\n"
        }
      ],
      "postBuildScripts" : [
        {
          "script" : "\"${BUILD_DIR%\/Build\/*}\/SourcePackages\/checkouts\/firebase-ios-sdk\/Crashlytics\/run\"\n",
          "inputFiles" : [
            "${DWARF_DSYM_FOLDER_PATH}\/${DWARF_DSYM_FILE_NAME}\/Contents\/Resources\/DWARF\/${TARGET_NAME}",
            "$(SRCROOT)\/$(BUILT_PRODUCTS_DIR)\/$(INFOPLIST_PATH)"
          ]
        }
      ]
    },
    "SmartAppUnitTests" : {
      "sources" : [
        {
          "excludes" : [
            "Info.plist"
          ],
          "path" : "..\/SmartAppUnitTests"
        }
      ],
      "templates" : [
        "UnitTest"
      ],
      "dependencies" : [
        {
          "sdk" : "XCTest.framework"
        },
        {
          "target" : "SmartApp",
          "link" : true
        },
        {
          "target" : "Common",
          "link" : false
        },
        {
          "target" : "Domain",
          "link" : false
        },
        {
          "link" : false,
          "target" : "Core"
        },
        {
          "package" : "Nimble",
          "product" : "Nimble",
          "link" : true
        },
        {
          "package" : "ViewInspector",
          "product" : "ViewInspector",
          "link" : true
        }
      ]
    }
  },
  "targetTemplates" : {
    "Framework" : {
      "platform" : "iOS",
      "settings" : {
        "base" : {
          "MARKETING_VERSION" : 1
        }
      },
      "type" : "framework",
      "deploymentTarget" : "16.0"
    },
    "UnitTest" : {
      "platform" : "iOS",
      "type" : "bundle.unit-test",
      "deploymentTarget" : "16.0",
      "settings" : ""
    },
    "UITest" : {
      "settings" : "",
      "type" : "bundle.ui-testing",
      "platform" : "iOS",
      "deploymentTarget" : "16.0"
    }
  },
  "options" : {
    "generateEmptyDirectories" : true,
    "bundleIdPrefix" : "com.ricardo.app.id",
    "groupSortPosition" : "top",
    "findCarthageFrameworks" : false,
    "minimumXcodeGenVersion" : "2.18.0",
    "deploymentTarget" : "16.0",
    "xcodeVersion" : "15.0.1"
  },
  "fileGroups" : [
    "..\/_Documents",
    "..\/Configuration",
    "..\/XcodeGen"
  ],
  "configs" : {
    "QA Debug" : "debug",
    "Dev Debug" : "debug",
    "Dev Release" : "release",
    "Production Debug" : "debug",
    "QA Release" : "release",
    "Production Release" : "release"
  },
  "name" : "SmartApp",
  "packages" : {
    "Firebase" : {
      "url" : "https:\/\/github.com\/firebase\/firebase-ios-sdk.git",
      "revision" : "888f0b6026e2441a69e3ee2ad5293c7a92031e62"
    },
    "Reachability" : {
      "revision" : "7b7018a69c84ea94ac2a38dff626e16ea81d1db9",
      "url" : "https:\/\/github.com\/ashleymills\/Reachability.swift"
    },
    "ViewInspector" : {
      "revision" : "7b1732802ffe30e6a67754bda6c7819e5cb0eb70",
      "url" : "https:\/\/github.com\/nalexn\/ViewInspector"
    },
    "Nimble" : {
      "url" : "https:\/\/github.com\/Quick\/Nimble",
      "branch" : "main"
    }
  },
  "aggregateTargets" : {
    "SwiftLint" : {
      "buildScripts" : [
        {
          "name" : "SwiftLint",
          "script" : "${SRCROOT}\/Scripts\/executeSwiftLint.sh\n",
          "shell" : "\/bin\/bash"
        }
      ]
    },
    "SwiftFormat" : {
      "buildScripts" : [
        {
          "name" : "SwiftFormat",
          "shell" : "\/bin\/bash",
          "script" : "${SRCROOT}\/Scripts\/executeSwiftFormat.sh\n"
        }
      ]
    }
  }
}